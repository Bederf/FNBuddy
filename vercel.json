{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10",
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
